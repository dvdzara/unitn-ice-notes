---
interface Props {
  graph: string;
}

const { graph } = Astro.props;
---

<details>
  <summary>Grafico</summary>

  <iframe
    src={`https://www.desmos.com/calculator/${graph}?embed`}
    style={`width: 300px; height: 300px;`}
    loading="lazy"
    title={graph}
  >
  </iframe>
</details>

<style>
  iframe {
    border: none;
  }
</style>
